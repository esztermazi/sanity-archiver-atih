<Window x:Class="SanityArchiver.DesktopUI.Views.RenameModal"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SanityArchiver.DesktopUI.Views"
        mc:Ignorable="d"
        Title="Rename File" Height="145" Width="498.667"
        KeyDown="Window_KeyDown"
        Topmost="True">
    <Grid Margin="0,0,0,4.667">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <TextBox x:Name="textField" HorizontalAlignment="Left" Height="23" Margin="96,27,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="370"/>
        <Label Content="Rename:" HorizontalAlignment="Left" Margin="36,23,0,0" VerticalAlignment="Top"/>
        <Button Content="Save" HorizontalAlignment="Left" Margin="289,81,0,0" VerticalAlignment="Top" Width="75" Click="SaveRename"/>
        <Button Content="Cancel" HorizontalAlignment="Left" Margin="391,81,0,0" VerticalAlignment="Top" Width="75" Click="CancelRename"/>

    </Grid>
</Window>
