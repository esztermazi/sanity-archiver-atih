<UserControl x:Class="SanityArchiver.DesktopUI.Views.TreeView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SanityArchiver.DesktopUI.Views"
             xmlns:Model="clr-namespace:SanityArchiver.Application.Models;assembly=SanityArchiver.Application"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <TreeView x:Name="CustomTreeview" ItemsSource="{Binding Items}">
        <TreeViewItem ItemsSource="{Binding Name}">
        </TreeViewItem>
    </TreeView>
</UserControl>
